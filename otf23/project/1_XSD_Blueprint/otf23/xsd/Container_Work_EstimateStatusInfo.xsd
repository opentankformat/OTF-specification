<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="Container_Work_EstimateStatuses.xsd"/>
  <xs:include schemaLocation="Container_ServiceProgress_AuthorizationInfo.xsd"/>
  <xs:include schemaLocation="Container_ServiceProgress_DateInfo.xsd"/>
  <xs:include schemaLocation="CRM_BusinessUnitInfo.xsd"/>   
  <xs:include schemaLocation="CRM_ContactInfo.xsd"/>   
  <xs:element name="Container_Work_EstimateStatusInfo">
    <xs:complexType>
      <xs:sequence>
		<xs:element name="ContainerWorkStatus" >
			<xs:simpleType>
				<xs:restriction base="Container_Work_EstimateStatuses" />
			</xs:simpleType>
		</xs:element>		  
		<xs:element name="Version" type="xs:string"  />
		<xs:element name="SupplierInfo" >
			<xs:complexType>
				<xs:sequence>
					<xs:element ref="Container_ServiceProgress_DateInfo" />
					<xs:element name="EstimatorInfo" >
						<xs:complexType>
							<xs:sequence>
								<xs:element ref="ContactInfo" />	  
							</xs:sequence>
						</xs:complexType>
					</xs:element>	
				</xs:sequence>
			</xs:complexType>
		</xs:element>	
		<xs:element name="LessorInfo" >
			<xs:complexType>
				<xs:sequence>
					<xs:element ref="BusinessUnitInfo" />	  
					<xs:element name="Reference" type="xs:string"  />
					<xs:element ref="Container_ServiceProgress_AuthorizationInfo"  />
					<xs:element name="AuthorizationAcceptedBySupplier" type="xs:dateTime" />						
				</xs:sequence>
			</xs:complexType>
		</xs:element>	
		<xs:element name="LesseeInfo" >
			<xs:complexType>
				<xs:sequence>
					<xs:element ref="BusinessUnitInfo" />	  
					<xs:element name="Reference" type="xs:string"  />
					<xs:element ref="Container_ServiceProgress_AuthorizationInfo"  />
					<xs:element name="AuthorizationAcceptedBySupplier" type="xs:dateTime" />						
				</xs:sequence>
			</xs:complexType>
		</xs:element>	
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>								