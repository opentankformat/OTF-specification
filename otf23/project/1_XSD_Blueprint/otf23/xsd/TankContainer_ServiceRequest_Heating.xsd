<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="Container_ServiceRequestInfo.xsd"/>
  <xs:include schemaLocation="TankContainer_HeatingMethods.xsd"/>
  <xs:include schemaLocation="TankContainer_HeatingTemperatureMeasurementMethods.xsd"/>
  <xs:include schemaLocation="Common_TemperatureMeasures.xsd"/>  
  <xs:element name="TankContainer_ServiceRequest_Heating">
    <xs:complexType>
      <xs:sequence>
		<xs:element ref="Container_ServiceRequestInfo" />
		<xs:element name="RequestedHeatingMethod"  >
			<xs:simpleType>
				<xs:restriction base="TankContainer_HeatingMethods" />
			</xs:simpleType>
		</xs:element>
		<xs:element name="RequestedHeatingMethodDescription" type="xs:string"  />
		<xs:element name="RequiredHeatingTemperatureMeasurementMethod"  >
			<xs:simpleType>
				<xs:restriction base="TankContainer_HeatingTemperatureMeasurementMethods" />
			</xs:simpleType>
		</xs:element>
		<xs:element name="RequiredHeatingTemperatureMeasurementMethodDescription" type="xs:string"  />
		<xs:element name="TargetTemperature" type="xs:double"  />
		<xs:element name="TargetTemperatureUnit"  >
			<xs:simpleType>
				<xs:restriction base="TemperatureMeasures" />
			</xs:simpleType>
		</xs:element>		
		<xs:element name="MaximumPressure" type="xs:double"  />			
		<xs:element name="MaximumPressureUnit" type="xs:string"  />
		<xs:element name="MaximumContactTemperature" type="xs:double"  />	
		<xs:element name="MaximumContactTemperatureUnit"  >
			<xs:simpleType>
				<xs:restriction base="TemperatureMeasures" />
			</xs:simpleType>
		</xs:element>			
		<xs:element name="MinimumTemperature" type="xs:double"  />			
		<xs:element name="MinimumTemperatureUnit"  >
			<xs:simpleType>
				<xs:restriction base="TemperatureMeasures" />
			</xs:simpleType>
		</xs:element>				
		<xs:element name="MaximumTemperature" type="xs:double"  />
		<xs:element name="MaximumTemperatureUnit"  >
			<xs:simpleType>
				<xs:restriction base="TemperatureMeasures" />
			</xs:simpleType>
		</xs:element>			
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>								