<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" >
  <xs:include schemaLocation="Common_Remarks.xsd"/>
  <xs:element name="Container_Work_EstimateLineProposal">
    <xs:complexType>
      <xs:sequence>
		<xs:element name="LaborRate" type="xs:double" />
		<xs:element name="LaborHours" type="xs:double" />
		<xs:element name="LaborPrice" type="xs:double" />
		<xs:element name="MaterialPrice" type="xs:double" />
		<xs:element name="LumpSumPrice" type="xs:double" />	
		<xs:element name="LineSubtotal" type="xs:double" />
		<xs:element name="Quantity" type="xs:double" default="1" />
		<xs:element name="DoAction" type="xs:boolean" default = "true" />
		<xs:element name="LessorPercentage" type="xs:double" minOccurs="0" />
		<xs:element name="LesseePercentage" type="xs:double" minOccurs="0" />
		<xs:element name="LineTotal" type="xs:double" />	
		<xs:element name="Remarks" type="Remarks" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
